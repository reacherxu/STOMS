<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="upload" extends="json-default" namespace="/">		
    	<action name="fileUpload" class="FileUploadAction" method="fileUpload">
			<result type="json" name="success">
				<param name="includeProperties">
					jsonResult,actionStatus
				</param>
			</result>
		</action>
		
		<action name="inAccountFileUpload" class="FileUploadAction" method="inAccountFileUpload">
			<result type="json" name="success">
				<param name="includeProperties">
					jsonResult,actionStatus
				</param>
			</result>
		</action>
		
		<action name="acquireItemPictureList" class="FileUploadAction" method="acquireItemPictureList">
			<result type="json" name="success">
				<param name="includeProperties">
					jsonResult,actionStatus
				</param>
			</result>
		</action>
		
		<action name="acquireInAccountPictureList" class="FileUploadAction" method="acquireInAccountPictureList">
			<result type="json" name="success">
				<param name="includeProperties">
					jsonResult,actionStatus
				</param>
			</result>
		</action>
		
		<action name="deleteItemPic" class="FileUploadAction" method="deleteItemPic">
			<result type="json" name="success">
				<param name="includeProperties">
					jsonResult,actionStatus
				</param>
			</result>
		</action>
		
		<action name="deleteInAccountPicture" class="FileUploadAction" method="deleteInAccountPicture">
			<result type="json" name="success">
				<param name="includeProperties">
					jsonResult,actionStatus
				</param>
			</result>
		</action>
		
	</package>
</struts>