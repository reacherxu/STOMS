<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>

	<package name="Budget" extends="json-default" namespace="/">

		<action name="insertBudget" class="BudgetAction" method="insertBudget">
			<result type="json" name="success">
				<param name="includeProperties">
					jsonResult,actionStatus
				</param>
			</result>
		</action>

		<action name="acquireContractID" class="BudgetAction" method="acquireContractID">
			<result type="json" name="success">
				<param name="includeProperties">
					jsonResult,actionStatus
				</param>
			</result>
		</action>
		<action name="updateBudget" class="BudgetAction" method="updateBudget">
			<result type="json" name="success">
				<param name="includeProperties">
					jsonResult,actionStatus
				</param>
			</result>
		</action>

		<action name="acquireBudgetContrast" class="BudgetAction" method="acquireBudgetContrast">
			<result type="json" name="success">
				<param name="includeProperties">
					jsonResult,actionStatus
				</param>
			</result>
		</action>
		<action name="findDetail" class="BudgetAction" method="findDetail">
			<result type="json" name="success">
				<param name="includeProperties">
					jsonResult,actionStatus
				</param>
			</result>
		</action>
		
		
		<action name="acquireBudgetMappingExpenditure" class="BudgetMappingAction"
			method="acquireBudgetMappingExpenditure">
			<result type="json" name="success">
				<param name="includeProperties">
					jsonResult,actionStatus
				</param>
			</result>
		</action>

		<action name="saveMapping" class="BudgetMappingAction" method="saveMapping">
			<result type="json" name="success">
				<param name="includeProperties">
					jsonResult,actionStatus
				</param>
			</result>
		</action>

		<action name="acquireMapping" class="BudgetMappingAction"
			method="acquireMapping">
			<result type="json" name="success">
				<param name="includeProperties">
					jsonResult,actionStatus
				</param>
			</result>
		</action>
	</package>
</struts>    
