* {
	margin: 0px;
	padding: 0px;
}

body {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
	background-color: #FFFFFF;
	text-align: center;
	
}

table td {
	height: 30px;
	font-size: 14px;
}

.leftLabel {
	width:17%;
	text-align: right;
	padding-right: 1em;
}
		
.rightContent {
	width:25%;
	text-align: left;
}

input[type="text"], input[type="password"] {
	height:20px;
	width:120px;
}